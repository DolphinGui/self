  "version": "0.2.0",
  "configurations": [
    {
      "type": "lldb",
      "request": "launch",
      "name": "selfc",
      "program": "${workspaceFolder}/build/selfc",
      "args": ["test2.me", "libselfstd.a"],
      "cwd": "${workspaceFolder}/build"
    },
    {
      "type": "gdb",
      "request": "launch",
      "name": "FFI_test",
      "target": "./ffi_test",
      "cwd": "${workspaceRoot}/build",
      "valuesFormatting": "parseText"
    },
    {
      "type": "gdb",
      "request": "launch",
      "name": "clang_parse",
      "target": "./clang_parse",
      "cwd": "${workspaceRoot}/build",
      "valuesFormatting": "parseText"
    },
    {
      "type": "gdb",
      "request": "launch",
      "name": "a.out",
      "target": "./a.out",
      "cwd": "${workspaceRoot}/build",
      "valuesFormatting": "parseText"
    },
    {
      "name": "lex_test",
      "type": "gdb",
      "request": "launch",
      "target": "./lex_test",
      "cwd": "${workspaceRoot}/build",
      "valuesFormatting": "prettyPrinters"
    },
    {
      "name": "integration_test",
      "type": "gdb",
      "request": "launch",
      "target": "./integration_test",
      "cwd": "${workspaceRoot}/build",
      "valuesFormatting": "prettyPrinters"
    }
  ]
}